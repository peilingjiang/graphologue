{
  "name": "graphologue",
  "version": "0.3.0",
  "private": true,
  "author": "Peiling Jiang",
  "description": "Use GPT-4 to stream diagrams, instead of tokens, in real-time!",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.9",
    "@mui/material": "^5.14.10",
    "dagre": "^0.8.5",
    "history": "^5.3.0",
    "openai": "^4.8.0",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-fast-compare": "^3.2.2",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.16.0",
    "react-spinners": "^0.13.8",
    "reactflow": "^11.8.3",
    "sass": "^1.67.0",
    "tinycolor2": "^1.6.0",
    "uuid": "^9.0.1"
  },
  "scripts": {
    "start": "REACT_APP_DEV_IDE=code react-scripts start",
    "start:jet": "REACT_APP_DEV_IDE=jet react-scripts start",
    "start:netlify": "netlify dev",
    "start:server": "python server/main.py",
    "build": "GENERATE_SOURCEMAP=false REACT_APP_DEV_IDE=code react-scripts build",
    "start:electron": "ELECTRON_START_URL=http://localhost:3000 && electron --trace-warnings ./public/electron.js",
    "electron": "PUBLIC_URL=./ npm run build && electron-builder",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,html,scss}\"",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/dagre": "^0.7.49",
    "@types/jest": "^29.5.5",
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.22",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^18.2.7",
    "@types/tinycolor2": "^1.4.4",
    "@types/uuid": "^9.0.4",
    "electron": "^26.3.0",
    "electron-builder": "^24.6.4",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "netlify-cli": "^16.3.6",
    "prettier": "^3.0.3",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,html,scss}": "prettier --write"
  },
  "build": {
    "appId": "app.graphologue",
    "productName": "Graphologue",
    "mac": {
      "icon": "./build/mac.icns"
    },
    "files": [
      "./build/**/*"
    ]
  }
}
